build:
	pip install ecdsa -t .
	pip install requests -t .
	pip install requests_hawk -t .
	pip install cryptography -t .
	pip install sops -t .
	zip -r autograph-lambda-python-healthcheck.zip *
clean:
	rm -rf autograph-lambda-python-healthcheck.zip mohawk* six* ecdsa* requests* sops* cryptography*

